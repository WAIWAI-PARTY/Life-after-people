config_version=4

[application]

config/name="Life after people"
run/main_scene="res://SavedWorld/normalWorld.tscn"
config/icon="res://imports/GuttyKreum_Osakav5/OsakaRPGMakerExample2_1088x832.png"

[autoload]

PlayerStats="*res://Player/PlayerStats.tscn"

[debug]

settings/fps/force_fps=144

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1920
window/size/test_height=1080
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
mouse_cursor/custom_image="res://UI/crosshair.png"
mouse_cursor/custom_image_hotspot=Vector2( 24, 24 )
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

icon=false

